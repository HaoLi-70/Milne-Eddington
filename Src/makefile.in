
src_dir=Src/


all: MEINVERSION

SRC := $(wildcard $(src_dir)*.c)
OBJS := $(SRC:.c=.o)

%.o: %.c
	$(CMP) $(CFLAGS) $@ $< $(IFLAGS)

MEINVERSION: $(OBJS)
	$(CMP) -o MEINVERSION $(OBJS) -L. $(LFLAGS)  -lm

.PHONY:clean

clean:
	find . -maxdepth 2 -name "*.o" -delete 
	find . -maxdepth 2 -name "MEINV" -delete 
